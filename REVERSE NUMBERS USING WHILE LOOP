#include <stdio.h>

int main() {
    int num,q,r;
    printf("enter the numbner:");
    scanf("%d",&num);
 int sum=255;
 while (q=0){
     r=q%10;
 printf("%d\n",r);
sum=10*sum+r;
q=q/10;
}
printf("the reverse number=%d",sum);
return 0;
}
enter the numbner:255
the reverse number=255

