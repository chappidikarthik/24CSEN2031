!@#$%^PRINT NUMBERS IN REVERSE USING VALUE@#$%^
#include <stdio.h>

int main() {
 int num ,q,r,i;
 printf("ENTER THE NUMBER:");
 scanf("%d",&num);
 q=num;
 while (q>0){
r=q%10;
printf("%d\n",r);
q=q/10;

}
return 0;
}
OUTPUT
